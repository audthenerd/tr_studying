<p id="notice"><%= notice %></p>

<h1>Travela</h1>
<%= link_to 'Add a New Trip', new_trip_path %>
<table>
	<thead>
		<tr>
			<th>Trip Name</th>
			<th>Description</th>
			<th>Start Date</th>
			<th>End Date</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @trips.each do |trip| %>
			<tr>
				<td><%= trip.title %></td>
				<td><%= trip.description %></td>
				<td><%= trip.start_date %></td>
				<td><%= trip.end_date %></td>
				<td><%= link_to 'View Trip', trip_places_path(trip) %></td>
			</tr>
		<% end %>
	</tbody>
</table>

<br>
<%= javascript_pack_tag 'hello_react' %>
<%= javascript_include_tag 'maps' %>
